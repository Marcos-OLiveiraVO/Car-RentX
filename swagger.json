          "type": "string"
        }
      }
    },
    "Car": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "brand": {
          "type": "string"
        },
        "category_id": {
          "type": "string"
        },
        "daily_rate": {
          "type": "number"
        },
        "licence_plate": {
          "type": "string"
        },
        "fine_amount": {
          "type": "number"
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "BearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
